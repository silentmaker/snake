{
  "name": "snake",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://silentmaker.github.io/snake",
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "p5": "^0.6.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-fastclick": "^3.0.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-scss": "node-sass-chokidar src/ -o src/",
    "watch-scss": "yarn build-scss && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "react-scripts build && gh-pages -d /build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  }
}
